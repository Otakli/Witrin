                                <li>
                                    <div class="cart-item product-summary">

                                    @if (ViewData["uyeurun"] == null)
                                    {
                                        <div class="price">Sepet Boş</div>

                                    }
                                    else {

                                        ViewBag.sepeturun = ViewData["uyeurun"];
                                        
                                        foreach(var item in ViewBag.sepeturun)
                                        {
                                            <div class="row">
                                                <div class="col-xs-4">
                                                    <div class="image">
                                                        <a href="#"><img src=@item.Urunler.Urunresimler.kucukresim alt=""></a>
                                                    </div>
                                                </div>
                                                <div class="col-xs-7">

                                                    <h3 class="name"><a href="index.php?page-detail">@item.Urunler.urun_ad</a></h3>
                                                    <div class="price">@item.urun_fiyat ₺</div>
                                                </div>
                                                <div class="col-xs-1 action">
                                                    <a href="#"><i class="fa fa-trash"></i></a>
                                                </div>
                                            </div>
                                        }

                                    }

                                   

                                        
